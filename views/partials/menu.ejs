<nav id="header" class="navbar navbar-expand-lg navbar-dark">
  <a class="navbar-brand ml-2" href="/">The Spectrum</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbarCombine"
    aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-right navbar-collapse navbarCombine" id="navbarTogglerDemo01">
    <ul class="navbar-nav ml-auto mt-2 mt-lg-0">
      <li class="nav-item">
        <a class="nav-link" href="/">Home </a>
      </li>
      <% if(user){ %>
      <li class="nav-item" id="logout">
        <a class="nav-link" href="/dash">Dashboard</a>
      </li>
      <li class="nav-item" id="logout">
        <a class="nav-link" href="/auth/logout">Logout</a>
      </li>
      <% if(user[0].id === '0'){ %>
        <li class="nav-item" id="logout">
          <a class="nav-link" href="/auth/admin">Admin</a>
        </li>
      <% } %>
      <% } else{ %>
      <li class="nav-item" id="login">
        <a class="nav-link" href="/auth/login">Log In</a>
      </li>
      <li class="nav-item" id="signup">
        <a class="nav-link" href="/auth/signup">Sign Up</a>
      </li>
      <% } %>

    </ul>
  </div>
</nav>
